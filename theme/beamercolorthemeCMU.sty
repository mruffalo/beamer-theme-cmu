\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{fg=cmured,bg=cmugray!20!white}
\setbeamercolor{frametitle right}{bg=cmugray!80!white}

\setbeamercolor*{separation line}{}
